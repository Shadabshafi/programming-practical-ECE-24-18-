#include <stdio.h>

// Macro to define a constant
#define PI 3.14159

// Macro for a function to calculate the square of a number
#define SQUARE(x) ((x) * (x))

// Macro to find the maximum of two numbers
#define MAX(a, b) ((a) > (b) ? (a) : (b))

int main() {
    // Using the PI constant macro
    printf("Value of PI: %.5f\n", PI);

    // Using the SQUARE macro to calculate the square of a number
    int num = 5;
    printf("Square of %d is: %d\n", num, SQUARE(num));

    // Using the MAX macro to find the maximum of two numbers
    int a = 10, b = 20;
    printf("Maximum of %d and %d is: %d\n", a, b, MAX(a, b));

    return 0;
}
