#include <stdio.h>

// Function to swap two numbers (call-by-value)
void swap(int a, int b) {
    int temp;
    temp = a;
    a = b;
    b = temp;

    // Display swapped values inside the function (for demonstration)
    printf("Inside function: a = %d, b = %d\n", a, b);
}

int main() {
    int x, y;

    // Input the numbers to be swapped
    printf("Enter two numbers: ");
    scanf("%d %d", &x, &y);

    // Display values before swapping
    printf("Before swapping: x = %d, y = %d\n", x, y);

    // Call the swap function
    swap(x, y);

    // Display values after calling the swap function
    printf("After swapping (in main function): x = %d, y = %d\n", x, y);

    return 0;
}
