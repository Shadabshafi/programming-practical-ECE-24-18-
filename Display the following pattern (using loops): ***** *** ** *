#include <stdio.h>

int main() {
    int i, j;

    // Outer loop for each line
    for(i = 5; i >= 1; i--) {
        // Inner loop for printing stars in each line
        for(j = 1; j < i; j++) {
            printf("*");
        }
        // Move to the next line after printing stars
        printf("\n");
    }

    return 0;
}
