#include <stdio.h>

// Function to calculate Fibonacci number using recursion
int fibonacci(int n) {
    if (n == 0) {
        return 0;  // Base case: Fibonacci(0) = 0
    } else if (n == 1) {
        return 1;  // Base case: Fibonacci(1) = 1
    } else {
        // Recursive case: Fibonacci(n) = Fibonacci(n-1) + Fibonacci(n-2)
        return fibonacci(n - 1) + fibonacci(n - 2);
    }
}

int main() {
    int n;

    // Input the number of terms to display
    printf("Enter the number of terms: ");
    scanf("%d", &n);

    // Display Fibonacci series up to n terms
    printf("Fibonacci series up to %d terms: \n", n);
    for (int i = 0; i < n; i++) {
        printf("%d ", fibonacci(i));  // Print Fibonacci(i) for each i from 0 to n-1
    }
    printf("\n");

    return 0;
}
