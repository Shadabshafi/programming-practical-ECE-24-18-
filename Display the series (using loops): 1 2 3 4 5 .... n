#include <stdio.h>

int main() {
    int n;

    // Input the value of n
    printf("Enter a number (n): ");
    scanf("%d", &n);

    // Using a for loop to display the series from 1 to n
    for (int i = 1; i <= n; i++) {
        printf("%d ", i);
    }

    printf("\n");  // Print a newline after the series
    return 0;
}
