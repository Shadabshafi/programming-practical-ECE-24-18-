#include <stdio.h>

int main() {
    int i, j;

    
    for(i = 1; i <= 3; i++) {
        
        for(j = 1; j <= 3 - i; j++) {
            printf(" ");
        }
    
        for(j = 1; j <= (2 * i - 1); j++) {
            if (j % 2 != 0) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        
        printf("\n");
    }

    return 0;
}
