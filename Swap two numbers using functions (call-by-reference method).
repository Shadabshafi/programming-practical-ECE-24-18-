#include <stdio.h>
void swap(int *x, int *y) {
    int temp = *x; 
    *x = *y;     
    *y = temp;     
}

int main() {
    int a, b;

    
    printf("Enter two numbers:\n");
    printf("a: ");
    scanf("%d", &a);
    printf("b: ");
    scanf("%d", &b);


    printf("\nBefore swapping:\n");
    printf("a = %d, b = %d\n", a, b);

    swap(&a, &b);

    printf("\nAfter swapping:\n");
    printf("a = %d, b = %d\n", a, b);

    return 0;
}
