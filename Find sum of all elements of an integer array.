#include <stdio.h>

int main() {
    int n, sum = 0;

    // Input the number of elements in the array
    printf("Enter the number of elements: ");
    scanf("%d", &n);

    int arr[n];

    // Input the elements of the array
    printf("Enter the elements:\n");
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Calculate the sum of elements
    for(int i = 0; i < n; i++) {
        sum += arr[i];
    }

    // Output the sum
    printf("Sum of all elements: %d\n", sum);

    return 0;
}
