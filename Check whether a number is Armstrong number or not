#include <stdio.h>
#include <math.h>

int main() {
    int number, sum = 0, temp, remainder, digits = 0;

    
    printf("Enter a number: ");
    scanf("%d", &number);

    temp = number;

    
    while (temp != 0) {
        temp /= 10;
        digits++;
    }

    temp = number;

    
    while (temp != 0) {
        remainder = temp % 10;
        sum += pow(remainder, digits);
        temp /= 10; 
    }

    
    if (sum == number) {
        printf("%d is an Armstrong number.\n", number);
    } else {
        printf("%d is not an Armstrong
